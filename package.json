{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build:client": "rm -rf build && cd ./client && npm run build --prod && cp -r build ../",
    "start": "ts-node index.ts",
    "start:server-dev": "NODE_ENV=server ts-node-dev server/index.ts",
    "start:dev": "NODE_ENV=development ts-node-dev server/index.ts",
    "start:prod": "NODE_ENV=production ts-node server/index.ts",
    "start:db": "heroku pg:psql -a hivesights",
    "eslint": "eslint --ext .ts .",
    "eslint-fix": "eslint --fix --ext .ts .",
    "eslint-file:": "eslint",
    "eslint-file-fix:": "eslint --fix",
    "deploy": "git add . && git commit -m deploying #deploy #minor && git push",
    "deploy:full": "npm run build:client && git add . && git commit -m #deploy uibuild_deploy && npm run deploy",
    "cypress:open": "npx cypress open",
    "test:e2e": "NODE_ENV=test cypress run",
    "test:int": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles",
    "test:int-company": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles company.test.ts",
    "test:int-review": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles review.test.ts"
  },
  "keywords": [],
