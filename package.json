    "@typescript-eslint/parser": "^4.24.0",
    "cypress": "^7.3.0",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
